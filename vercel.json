{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend_static/(.*)",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "frontend_static/$1"
        }
    ],
    "outputDirectory": "frontend_static"
}