{
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend_static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    },
    {
      "src": "/",
      "dest": "frontend_static/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "frontend_static/$1"
    }
  ]
}
